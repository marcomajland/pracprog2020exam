set term svg background rgb 'white'
set out "Plot.svg"
set xlabel "y"
set ylabel "x"
set xzeroaxis
set yzeroaxis
set tics out
set key bottom right
set style circle radius 0.025
set title "Artificial neural network used to fit derivative of y = sin(x) with Gaussians as activation functions"
plot "./datafiles/plot.txt" using 1:2 with lines lc rgbcolor "black" title "ANN fit", "./datafiles/derivative.txt" using 1:2 with circles fill solid lc rgbcolor "blue" title "Tabulated points of y = cos(x)"

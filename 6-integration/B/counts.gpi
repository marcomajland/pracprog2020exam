set term svg
set out "CountsPlot.svg"
set xlabel "Absolute accuracy"
set ylabel "Integration counts"
set xzeroaxis
set yzeroaxis
set tics out
set key top left
set xrange [*:*] reverse
set log xy
plot "counts_out.txt" using 1:2 with lines title "Trapezoidal quadrature (without CC)", "counts_out.txt" using 1:3 with lines title "Trapezoidal quadrature (with CC)", "counts_out.txt" using 1:4 with lines title "o8av"

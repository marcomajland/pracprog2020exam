set term svg
set out "AccuracyPlot.svg"
set xlabel "Absolute accuracy"
set ylabel "Error |Q-pi|"
set xzeroaxis
set yzeroaxis
set tics out
set key top right
set xrange [*:*] reverse
set log xy
plot "error_out.txt" using 1:2 with lines title "Trapezoidal quadrature (without CC)", "error_out.txt" using 1:3 with lines title "Trapezoidal quadrature (with CC)", "error_out.txt" using 1:4 with lines title "o8av"

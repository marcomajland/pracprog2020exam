set term svg
set out "Lspline_int.svg"
set xlabel "x"
set ylabel "y"
set xzeroaxis
set yzeroaxis
set tics out
set key bottom right
set style circle radius 0.045
plot "data.txt" using 1:2 with circles fill solid title "y = x^2", "lspline_out.txt" using 1:3 with lines title "Linear interpolation integral"

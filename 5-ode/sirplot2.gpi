set term svg
set out "SIRPlot2.svg"
set xlabel "Time [days]"
set ylabel "Number of individuals [million]"
set xzeroaxis
set yzeroaxis
set tics out
set key bottom right
plot "sir_out.txt" using 1:2 with lines title "Susceptible", "sir_out.txt" using 1:3 with lines title "Infectious", "sir_out.txt" using 1:4 with lines title "Removed"

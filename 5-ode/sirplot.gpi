set term svg
set out "SIRPlot.svg"
set xlabel "Time [days]"
set ylabel "Number of individuals [million]"
set xzeroaxis
set yzeroaxis
set tics out
set key bottom right
plot "sir_out_T_c_0.25.txt" using 1:2 with lines title "Susceptible", "sir_out_T_c_0.25.txt" using 1:3 with lines title "Infectious", "sir_out_T_c_0.25.txt" using 1:4 with lines title "Removed", "sir_out_T_c_0.75.txt" using 1:2 with lines linestyle 0 lc rgbcolor "red" title "Susceptible 0.75", "sir_out_T_c_0.75.txt" using 1:3 with lines linestyle 0 lc rgbcolor "red" title "Infectious 0.75", "sir_out_T_c_0.75.txt" using 1:4 with lines linestyle 0 lc rgbcolor "red" title "Removed 0.75"
